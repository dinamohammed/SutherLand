<odoo>
  <data>
    <record id="view_employee_form_inherit_insurance_date" model="ir.ui.view" >
        <field name="name">hr.employee.form.insurance.date.inherit</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='hr_settings']/group" position="after">
                    <div>
                        <field name="day_seperate" placeholder="Day..."/>
                        <field name="month_seperate" placeholder="Month..."/>
                        <field name="year_seperate" placeholder="Year..."/>
                    </div>
            </xpath>
        </field>
    </record>
  </data>
</odoo>