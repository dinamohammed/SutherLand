<odoo>
  <data>
    <record id="view_employee_form_inherit_insurance_date" model="ir.ui.view" >
        <field name="name">hr.employee.form.insurance.date.inherit</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='hr_settings']/group" position="after">
                <tree>
                    <field name="day_seperate"/>
                    <field name="month_seperate"/>
                    <field name="year_seperate"/>
                </tree>
            </xpath>

        </field>
    </record>
  </data>
</odoo>